{
  "name": "fullstack-test-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "predev": "npm run prisma:migrate",
    "dev": "nodemon src/index.js",
    "prestart": "npm run prisma:migrate",
    "start": "node src/index.js",
    "postinstall": "prisma generate --schema ../prisma/schema.prisma",
    "seed": "prisma db seed --schema ../prisma/schema.prisma",
    "prisma:migrate": "prisma migrate deploy --schema ../prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "5.21.1",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2"
  },
  "devDependencies": {
    "nodemon": "3.1.4",
    "prisma": "5.21.1"
  },
  "prisma": {
    "seed": "node ../prisma/seed.js"
  }
}
